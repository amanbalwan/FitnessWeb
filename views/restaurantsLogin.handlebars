
    
    {{#if error}}
    <p class="error">{{error}}</p>
    {{/if}}
    <form method="post" action="/restaurant/restaurantsLogin">
        <label for="storeEmailInput">Email Address:</label>
        <input type="email" id="storeEmailInput" name="storeEmailInput" value="{{storeEmailInput}}" required>
        <br>
        <label for="storePasswordInput">Password:</label>
        <input type="password" id="storePasswordInput" name="storePasswordInput" value="{{storePasswordInput}}"
            required>
        <br>
        <button type="submit" id="login_btn">Login</button>
    </form>
    
<footer>
  <nav>
    <ul>
      <li><a href="/restaurant/restaurantsSignup" id="a_for_register">Click here to register!</a></li>
      <li><a href="/loginuser">Login as User</a></li>
      <li><a href="/fitness/login">Login as Fitness owner</a></li>
      <li><a href="/dietitian/login">Login as Diteation</a></li>
    </ul>
  </nav>
</footer>